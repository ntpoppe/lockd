<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="AccentColor">#FF8C6D4F</Color>
    <Color x:Key="BackgroundColor">#FFF5F5F5</Color>
    <Color x:Key="ErrorColor">#FFC94C44</Color>
    <Color x:Key="InputBackgroundColor">#FFFFFFFF</Color>
    <Color x:Key="PrimaryAccentColor">#FFB38B5D</Color>
    <Color x:Key="PrimaryTextColor">#FF2A2A2A</Color>
    <Color x:Key="SecondaryAccentColor">#FFB5A68D</Color>
    <Color x:Key="SecondaryTextColor">#FF505050</Color>
    <Color x:Key="SubtitleColor">#FF6B6B6B</Color>
    <Color x:Key="TextBoxSelectedBackgroundColor">#FFF0E8E0</Color>
    <Color x:Key="TextBoxSelectedBorderColor">#FF8C6D4F</Color>
    <Color x:Key="UserProfileColor">#FF9F8772</Color>

    <SolidColorBrush x:Key="AccentBrush" Color="{StaticResource AccentColor}" />
    <SolidColorBrush x:Key="BackgroundBrush" Color="{StaticResource BackgroundColor}" />
    <SolidColorBrush x:Key="ErrorBrush" Color="{StaticResource ErrorColor}" />
    <SolidColorBrush x:Key="InputBackgroundBrush" Color="{StaticResource InputBackgroundColor}" />
    <SolidColorBrush x:Key="PrimaryAccentBrush" Color="{StaticResource PrimaryAccentColor}" />
    <SolidColorBrush x:Key="PrimaryTextBrush" Color="{StaticResource PrimaryTextColor}" />
    <SolidColorBrush x:Key="SecondaryAccentBrush" Color="{StaticResource SecondaryAccentColor}" />
    <SolidColorBrush x:Key="SecondaryTextBrush" Color="{StaticResource SecondaryTextColor}" />
    <SolidColorBrush x:Key="SubtitleBrush" Color="{StaticResource SubtitleColor}" />
    <SolidColorBrush x:Key="TextBoxSelectedBackgroundBrush" Color="{StaticResource TextBoxSelectedBackgroundColor}" />
    <SolidColorBrush x:Key="TextBoxSelectedBorderBrush" Color="{StaticResource TextBoxSelectedBorderColor}" />
    <SolidColorBrush x:Key="UserProfileBrush" Color="{StaticResource UserProfileColor}" />

</ResourceDictionary>

